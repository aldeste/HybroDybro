{
  "ecmaVersion": 8,
  "libs": [
    "browser"
  ],
  "loadEagerly": [
    "src/**/*.js"
  ],
  "dontLoad": [
    "server/**/*.js",
    "node_modules/"
  ],
  "plugins": {
    "complete_strings": {
      "maxLength": 15
    },
    "node": {},
    "doc_comment": {
      "fullDocs": true,
      "strong": true
    }
  }
}
